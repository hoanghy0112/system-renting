# Backend WebSocket URL
AGENT_BACKEND_URL=wss://api.yourplatform.com/fleet

# Agent API Key (obtained from platform dashboard)
AGENT_API_KEY=your_api_key_here

# Node ID (assigned after registration)
NODE_ID=

# FRP Server Configuration
FRP_SERVER_ADDR=proxy.yourplatform.com
FRP_SERVER_PORT=7000
FRP_TOKEN=your_frp_token_here

# Optional: Path to frpc binary (defaults to bundled or PATH)
FRPC_PATH=

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
